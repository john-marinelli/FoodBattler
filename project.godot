"class": "Ability",
"language": "GDScript",
"path": "res://Entities/Monsters/Abilities/Ability.gd"
}, {
"base": "Node",
"class": "Encounter",
"language": "GDScript",
"path": "res://BattleSystem/Encounter.gd"
}, {
"base": "Node",
"class": "Monster",
"language": "GDScript",
"path": "res://Entities/Monsters/Base/scripts/Monster.gd"
} ]
_global_script_class_icons={
"Ability": "",
"Encounter": "",
"Monster": ""
}

[application]

config/name="FoodBattler"
config/icon="res://icon.png"

[autoload]

Globals="*res://Globals/Globals.gd"

[display]

window/size/width=320
window/size/height=180
window/stretch/mode="2d"
window/stretch/aspect="keep_height"

