; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "AIHandler",
"language": "GDScript",
"path": "res://Entities/AI/AIHandler.gd"
}, {
"base": "Node",
"class": "Ability",
"language": "GDScript",
"path": "res://Entities/Monsters/Abilities/Ability.gd"
}, {
"base": "Node",
"class": "Character",
"language": "GDScript",
"path": "res://Entities/Character.gd"
}, {
"base": "Node",
"class": "Encounter",
"language": "GDScript",
"path": "res://BattleSystem/Encounter.gd"
}, {
"base": "Node",
"class": "Monster",
"language": "GDScript",
"path": "res://Entities/Monsters/Base/scripts/Monster.gd"
}, {
"base": "Node",
"class": "RandomEncounter",
"language": "GDScript",
"path": "res://Entities/AI/RandomEncounter.gd"
}, {
"base": "Area2D",
